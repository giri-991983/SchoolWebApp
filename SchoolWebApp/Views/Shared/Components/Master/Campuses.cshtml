@using SchoolWebApp.Models
@{
    var selectedID = ViewBag.selectedIDs;
    var campuses = ViewBag.Campuses as List<Campus>;
}

<option value="">Select Campus</option>
@foreach (var campus in campuses)
{
    <option value="@campus.CampusID" selected="@(campus.CampusID == selectedID)">@campus.CampuseName</option>
}
